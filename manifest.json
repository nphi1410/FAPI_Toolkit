{
    "manifest_version": 3,
    "name":"FAPI",
    "description":"Beauty FAP Interface",
    "version":"1.0.0",
    "action": {
      "default_icon": "icon.png"
    },
    "permissions": ["scripting", "activeTab"],
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts":[
        {
            "matches": ["https://fap.fpt.edu.vn/Student.aspx"],
            "js":["backend/content.js", "front_end/script/content.js"],
            "css": ["front_end/MyStyle/myStyle.css", "front_end/MyStyle/main.css"]
        },
        {
            "matches": ["https://fap.fpt.edu.vn/Report/ScheduleOfWeek.aspx"],
            "js":["backend/extractData.js"]
        }
    ]
}